<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>CloudlessMVC</title>
    <parser>
        <target>data/docs</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>php3</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>data/docs</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>data/log/{DATE}.log</default>
            <errors>data/log/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <file>AoiSora.php</file>
        <file>init.php</file>
        <file>clmvc-autoloader.php</file>
        <directory>lib</directory>
        <ignore>tests/*</ignore>
    </files>
</phpdoc>